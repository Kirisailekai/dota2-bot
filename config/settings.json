{
    "system": {
        "name": "Dota 5 Bot System",
        "version": "1.0.0",
        "debug": true,
        "log_level": "INFO"
    },
    "sandbox": {
        "count": 5,
        "prefix": "DOTA_BOT_",
        "sandboxie_path": "C:/Program Files/Sandboxie-Plus",
        "working_dir": "C:/dota_bots"
    },
    "dota": {
        "game_path": "C:/Program Files (x86)/Steam/steamapps/common/dota 2 beta",
        "executable": "game/dota/bin/win64/dota2.exe",
        "launch_options": [
            "-novid",
            "-console",
            "-high",
            "+map dota",
            "+dota_bot_set_difficulty 1",
            "+dota_force_gamemode 1"
        ]
    },
    "ai": {
        "capture_interval": 0.1,
        "decision_interval": 0.5,
        "health_threshold": 30,
        "mana_threshold": 20,
        "confidence_threshold": 0.7,
        "model_path": "ai/trained_models"
    },
    "input": {
        "move_duration": 0.2,
        "click_delay": 0.1,
        "key_press_duration": 0.05,
        "scroll_duration": 0.1
    },
    "performance": {
        "max_cpu_percent": 80,
        "max_memory_mb": 4096,
        "target_fps": 30,
        "gpu_acceleration": true
    },
    "network": {
        "websocket_port": 8765,
        "sync_interval": 1.0,
        "timeout": 10
    },
    "accounts": {
        "file": "config/accounts.json",
        "auto_login": true,
        "steam_guard_timeout": 30
    }
}